{
  "name": "@shadow/temporal-integration",
  "version": "1.0.0",
  "description": "Comprehensive Temporal.io TypeScript SDK integration for Shadow platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "build:production": "tsc --build tsconfig.build.json",
    "build:webpack": "tsc --noEmitOnError false && webpack --config webpack.bundle.config.js",
    "bundle:analyze-worker": "webpack-bundle-analyzer dist/bundles/production-worker.bundle.js",
    "dev": "concurrently \"npm run build:watch\" \"nodemon dist/worker.js\"",
    "start": "node dist/worker.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "temporal:start": "temporal server start-dev",
    "temporal:ui": "open http://localhost:8233",
    "worker:dev": "nodemon --exec \"ts-node src/workers/dev-worker.ts\"",
    "worker:prod": "node dist/workers/production-worker.js",
    "bundle:workflows": "webpack --config webpack.workflows.config.js",
    "bundle:worker": "webpack --config webpack.worker.config.js",
    "bundle:analyze": "webpack-bundle-analyzer dist/workers/worker.bundle.js",
    "docker:build": "docker build -t shadow-temporal .",
    "docker:run": "docker run --rm -p 3000:3000 shadow-temporal",
    "deploy:staging": "./scripts/deploy-staging.sh",
    "deploy:production": "./scripts/deploy-production.sh",
    "generate:docs": "typedoc --out docs/api src",
    "monitor": "node dist/monitoring/dashboard.js"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.43.0",
    "@opentelemetry/exporter-jaeger": "^1.21.0",
    "@opentelemetry/exporter-prometheus": "^0.50.0",
    "@opentelemetry/resources": "^1.21.0",
    "@opentelemetry/sdk-trace-base": "^1.21.0",
    "@opentelemetry/sdk-trace-node": "^1.21.0",
    "@opentelemetry/semantic-conventions": "^1.21.0",
    "@temporalio/activity": "^1.11.3",
    "@temporalio/client": "^1.11.3",
    "@temporalio/common": "^1.11.3",
    "@temporalio/testing": "^1.11.3",
    "@temporalio/worker": "^1.11.3",
    "@temporalio/workflow": "^1.11.3",
    "axios": "^1.11.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.0",
    "protobufjs": "^7.2.5",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "concurrently": "^8.2.2",
    "docker": "^1.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "temporal",
    "workflow",
    "typescript",
    "distributed-systems",
    "microservices",
    "orchestration",
    "shadow-platform"
  ],
  "author": "Shadow Platform Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Git-Godssoldier/shadow-clean.git",
    "directory": "temporal-integration"
  }
}
